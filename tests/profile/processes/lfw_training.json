{
  "name": "Import LFW and train LOPQ approximator on LFW dataset",
  "description": "Train two LOPQ approximators on Labeled Faces in Wild dataset. This script imports LFW dataset with name LFW. Make sure there are no other datasets named LFW, otherwise please modify name in the script.",
  "process_type": "V",
  "create": [
    {
      "MODEL": "TrainingSet",
      "spec": {
        "name": "LFW_facenet_indexes",
        "training_task_type": "A",
        "instance_type": "X",
        "source_filters": {
          "indexer_shasum": "9f99caccbc75dcee8cb0a55a0551d7c5cb8a6836",
          "video__name": "LFW"
        }
      }
    },
    {
      "MODEL": "Video",
      "spec": {
        "name": "LFW",
        "url": "https://www.dropbox.com/s/6nn84z4yzy47vuh/LFW.dva_export.zip",
        "dataset": true
      },
      "tasks": [
        {
          "operation": "perform_import",
          "video_id": "__pk__",
          "arguments": {
            "map": [
              {
                "operation": "perform_training_set_creation",
                "arguments": {
                  "training_set_selector": {
                    "name": "LFW_facenet_indexes"
                  },
                  "map": [
                    {
                      "operation": "perform_training",
                      "arguments": {
                        "trainer": "LOPQ",
                        "sub": "256",
                        "indexer_shasum": "9f99caccbc75dcee8cb0a55a0551d7c5cb8a6836",
                        "m": "32",
                        "components": "128",
                        "v": "32",
                        "lopq_train_opts": {
                          "kmeans_coarse_iters": 10,
                          "kmeans_local_iters": 20,
                          "n_init": 10,
                          "subquantizer_sample_ratio": 1.0
                        },
                        "training_set_selector": {
                          "name": "LFW_facenet_indexes"
                        },
                        "name": "approx_32_32",
                        "map": [
                          {
                            "operation": "perform_index_approximation",
                            "video_selector": {
                              "name": "LFW"
                            },
                            "arguments": {
                              "target": "index_entries",
                              "approximator": "approx_32_32",
                              "filters": {
                                "indexer_shasum": "9f99caccbc75dcee8cb0a55a0551d7c5cb8a6836",
                                "approximate": false
                              }
                            }
                          }
                        ]
                      }
                    },
                    {
                      "operation": "perform_training",
                      "arguments": {
                        "trainer": "LOPQ",
                        "sub": "256",
                        "indexer_shasum": "9f99caccbc75dcee8cb0a55a0551d7c5cb8a6836",
                        "m": "64",
                        "components": "128",
                        "v": "64",
                        "lopq_train_opts": {
                          "kmeans_coarse_iters": 10,
                          "kmeans_local_iters": 20,
                          "n_init": 10,
                          "subquantizer_sample_ratio": 1.0
                        },
                        "training_set_selector": {
                          "name": "LFW_facenet_indexes"
                        },
                        "name": "approx_64_64",
                        "map": [
                          {
                            "operation": "perform_index_approximation",
                            "video_selector": {
                              "name": "LFW"
                            },
                            "arguments": {
                              "target": "index_entries",
                              "approximator": "approx_64_64",
                              "filters": {
                                "indexer_shasum": "9f99caccbc75dcee8cb0a55a0551d7c5cb8a6836",
                                "approximate": false
                              }
                            }
                          }
                        ]
                      }
                    }
                  ]
                }
              }
            ]
          }
        }
      ]
    }
  ]
}
